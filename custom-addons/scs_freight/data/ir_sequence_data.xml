<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Freight Operation -->
        <record id="seq_direct_freight_operation" model="ir.sequence">
            <field name="name">Direct Freight Operation</field>
            <field name="code">freight.operation.direct</field>
            <field name="prefix">DIR-</field>
            <field name="padding">4</field>
        </record>
        <!-- Sequence for Freight Container -->
        <record id="seq_freight_container" model="ir.sequence">
            <field name="name">Freight Container Sequence</field>
            <field name="code">freight.container.sequence</field>
            <field name="prefix">CN</field>
            <field name="padding">4</field>
        </record>
        <!-- Sequence for Freight Folder -->
        <record id="seq_freight_folder" model="ir.sequence">
            <field name="name">Freight Folder Sequence</field>
            <field name="code">freight.folder.sequence</field>
            <field name="prefix">F</field>
            <field name="padding">4</field>
        </record>

        <!-- Sequence for Freight offer -->
        <record id="seq_freight_offer" model="ir.sequence">
            <field name="name">Freight Offer Sequence</field>
            <field name="code">freight.offer.sequence</field>
            <field name="prefix">OF</field>
            <field name="padding">4</field>
        </record>
    </data>
</odoo>
